	<select id="queryAllBidHist" parameterClass="java.util.Map" resultMap="BidHistDto">
		select T.* from bid_hist T where
			T.BID_NO = #BID_NO#
			order by T.UPDATE_DATE desc
	</select>
	
	<select id="queryBidHistCountByPage" resultClass="int" parameterClass="java.util.Map">
		select count(*) from bid_hist T where
			T.BID_NO = #BID_NO#
	</select>
	
	<select id="queryBidHistByPage" parameterClass="java.util.Map" resultMap="BidHistDto">
		select T.* from bid_hist T where
			T.BID_NO = #BID_NO#
			order by T.UPDATE_DATE desc
			LIMIT #start#, #end#
	</select>
	
	<select id="queryBidHistByID" parameterClass="java.util.Map" resultMap="BidHistDto">
		select * from bid_hist where 
			BID_HIS_SEQ = #BID_HIS_SEQ#
	</select>
	
