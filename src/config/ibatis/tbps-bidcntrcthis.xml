	<select id="queryBidCntrctHisCountByPage" resultClass="int" parameterClass="java.util.Map">
		select count(*) from bidcntrct_hist T where 1 = 1
		<isNotEmpty prepend="and" property="CNTRCT_NAME">
			T.CNTRCT_NAME like '%$CNTRCT_NAME$%'
		</isNotEmpty>
		<isNotEmpty prepend="and" property="CNTRCT_TYPE">
			T.CNTRCT_TYPE = #CNTRCT_TYPE#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="CNTRCT_NO">
			T.CNTRCT_NO = #CNTRCT_NO#
		</isNotEmpty>
	</select>
	
	<select id="queryBidCntrctHisByPage" parameterClass="java.util.Map" resultMap="BidCntrctHisDto">
		select T.* from bidcntrct_hist T where 1 = 1
		<isNotEmpty prepend="and" property="CNTRCT_NAME">
			T.CNTRCT_NAME like '%$CNTRCT_NAME$%'
		</isNotEmpty>
		<isNotEmpty prepend="and" property="CNTRCT_TYPE">
			T.CNTRCT_TYPE = #CNTRCT_TYPE#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="CNTRCT_NO">
			T.CNTRCT_NO = #CNTRCT_NO#
		</isNotEmpty>
		order by BID_CNTRCT_HIS_SEQ desc
		LIMIT #start#, #end#
	</select>
	
	<select id="queryBidCntrctHisByID" parameterClass="java.util.Map" resultMap="BidCntrctHisDto">
		select * from bidcntrct_hist where 
			BID_CNTRCT_HIS_SEQ = #BID_CNTRCT_HIS_SEQ#
			order by BID_CNTRCT_HIS_SEQ desc
	</select>
	
