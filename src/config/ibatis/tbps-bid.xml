	<select id="queryBidAndBidCntrctCountByPage" resultClass="int" parameterClass="java.util.Map">
		select count(*) from (
			select * from (
				select C.BID_COMP_NO,C.CNTRCT_YEAR,C.CNTRCT_NAME,C.CNTRCT_ST_DATE,C.CNTRCT_ED_DATE,C.BID_COMP_NAME,
					T.* from bid T left join bidcntrct C on C.CNTRCT_NO = T.CNTRCT_NO
			) B where 1 = 1 and B.DELETE_FLG = '1'
			<isNotEmpty prepend="and" property="CNTRCT_YEAR">
				B.CNTRCT_YEAR = #CNTRCT_YEAR#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="CNTRCT_TYPE">
				B.CNTRCT_TYPE = #CNTRCT_TYPE#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="CNTRCT_NO">
				B.CNTRCT_NO = #CNTRCT_NO#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="BID_COMP_NO">
				B.BID_COMP_NO = #BID_COMP_NO#
			</isNotEmpty>
			<isNotEmpty prepend="and" property="CNTRCT_NAME">
				B.CNTRCT_NAME like '%$CNTRCT_NAME$%'
			</isNotEmpty>
			<isNotEmpty prepend="and" property="CNTRCT_ST_DATE">
