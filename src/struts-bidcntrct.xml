<struts>

	<package name="bidcntrct" extends="struts-tbps" namespace="/bidcntrct">
	
		<action name="queryBidCntrctAjax" class="bidCntrctAction" method="queryBidCntrctAjax">
			<result type="json"></result>
		</action>
	
		<action name="showBidCntrctPage" class="bidCntrctAction" method="showBidCntrctPage">
			<result name="success">/page/bidcntrct/bidcntrctlist.jsp</result>
		</action>
		
		<action name="queryBidCntrctList" class="bidCntrctAction" method="queryBidCntrctList">
			<result name="success">/page/bidcntrct/bidcntrctlist.jsp</result>
		</action>
		
		<action name="turnBidCntrctPage" class="bidCntrctAction" method="turnBidCntrctPage">
			<result name="success">/page/bidcntrct/bidcntrctlist.jsp</result>
		</action>
		
		<action name="delBidCntrct" class="bidCntrctAction" method="delBidCntrct">
			<result name="success">/page/bidcntrct/bidcntrctlist.jsp</result>
		</action>
		
		<action name="showAddBidCntrct" class="bidCntrctAction" method="showAddBidCntrct">
			<result name="success">/page/bidcntrct/addbidcntrct.jsp</result>
		</action>
		
		<action name="addBidCntrct" class="bidCntrctAction" method="addBidCntrct">
			<result name="success">/page/bidcntrct/addbidcntrct.jsp</result>
			<result name="checkerror">/page/bidcntrct/addbidcntrct.jsp</result>
		</action>
		
		<action name="showUpdBidCntrct" class="bidCntrctAction" method="showUpdBidCntrct">
			<result name="success">/page/bidcntrct/updbidcntrct.jsp</result>
		</action>
		
		<action name="updBidCntrct" class="bidCntrctAction" method="updBidCntrct">
			<result name="success">/page/bidcntrct/updbidcntrct.jsp</result>
			<result name="checkerror">/page/bidcntrct/updbidcntrct.jsp</result>
		</action>
		
	</package>

</struts>